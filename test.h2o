fonksiyon test(sayi1, sayi2){
    eger sayi1 esittir sayi2
        yazdir "sayi1 == sayi2"
    bitir

    eger sayi1 !esittir sayi2
        yazdir "sayi1 != sayi2"
    bitir

    eger sayi1 kucuktur sayi2
        yazdir "sayi1 < sayi2"
    bitir

    eger sayi1 kucukesittir sayi2
        yazdir "sayi1 <= sayi2"
    bitir

    eger sayi1 buyuktur sayi2
        yazdir "sayi1 > sayi2"
    bitir

    eger sayi1 buyukesittir sayi2
        yazdir "sayi1 >= sayi2"
    bitir
}

kullan test("12", "23")
